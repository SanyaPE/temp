{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
},
  "parser": "@babel/eslint-parser",
  "extends": [
    // "eslint:recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "requireConfigFile": false
  },
  "plugins": [
    "prettier"
],
  "rules": {
    "prettier/prettier": [
      "error",
      {
          "endOfLine":"auto"
      }
  ],
    "no-console":0,
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off"
  }
}
